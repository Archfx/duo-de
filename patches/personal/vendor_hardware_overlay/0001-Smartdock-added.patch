From 92b0a37fa9ae6bc3c8f3bc9218fcb61e675bbee1 Mon Sep 17 00:00:00 2001
From: Archfx <cybx@live.com>
Date: Wed, 12 Jun 2024 11:55:08 -0400
Subject: [PATCH] Smartdock added

---
 Smartdock/Android.mk | 17 +++++++++++++++++
 overlay.mk           |  1 +
 2 files changed, 18 insertions(+)
 create mode 100644 Smartdock/Android.mk

diff --git a/Smartdock/Android.mk b/Smartdock/Android.mk
new file mode 100644
index 0000000..327a93a
--- /dev/null
+++ b/Smartdock/Android.mk
@@ -0,0 +1,17 @@
+LOCAL_PATH := $(my-dir)
+
+include $(CLEAR_VARS)
+
+LOCAL_MODULE := Smartdock
+LOCAL_SRC_FILES := app.apk
+LOCAL_MODULE_CLASS := APPS
+LOCAL_MODULE_SUFFIX := .apk
+LOCAL_BUILT_MODULE_STEM := package.apk
+LOCAL_CERTIFICATE := platform
+LOCAL_PRIVILEGED_MODULE := true
+LOCAL_OVERRIDES_PACKAGES := Trebuchet Home Launcher2 Launcher3 Launcher3QuickStep
+ifeq ($(shell test $(PLATFORM_SDK_VERSION) -ge 31 && echo OK),OK)
+LOCAL_ENFORCE_USES_LIBRARIES := false
+endif
+
+include $(BUILD_PREBUILT)
diff --git a/overlay.mk b/overlay.mk
index 7bf0ead..1934072 100644
--- a/overlay.mk
+++ b/overlay.mk
@@ -1,4 +1,5 @@
 PRODUCT_PACKAGES += \
+	Smartdock \
 	HardwareOverlayPicker \
 	QtiAudio \
 	TrebleApp \
-- 
